@model IEnumerable<AssognmentEnterprice.Models.Genre>

@{
    ViewBag.Title = "Genre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Genres</h2>

<ul>
    @foreach (var item in Model) 

        {
        <li>
            @Html.ActionLink(item.Name, "Details", new { id = item.GenreId })
        </li>
        }
</ul>




